<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="helloWorld" class="test.springevent.HelloWorld">
        <property name="message" value="Test String"></property>
    </bean>

    <bean id="cStartEventHandler" class="test.springevent.CStartEventHandler"></bean>

    <bean id="cStopEventHandler" class="test.springevent.CStopEventHandler"></bean>

</beans>